{
  "name": "epicgames-freegames-node",
  "version": "4.0.0",
  "description": "Use Epic's web APIs to login and redeem free games.",
  "main": "src/index.ts",
  "scripts": {
    "start": "node dist/src/index.js",
    "start:ts": "DEBUG=puppeteer-extra-plugin:portal* ts-node src/index.ts",
    "entrypoint-config": "node dist/src/entrypoint-config.js",
    "build": "rimraf dist && tsc",
    "lint": "tsc --noEmit && eslint **/*.ts",
    "test": "jest",
    "redeem": "ts-node commands.ts redeem",
    "create": "ts-node commands.ts create",
    "docker:build": "docker build . -t charlocharlie/epicgames-freegames-node:latest --target deploy",
    "docker:build-debian": "docker build . -t charlocharlie/epicgames-freegames-node:bullseye-slim --target deploy --file Dockerfile.debian",
    "docker:build-dev": "docker build . -t charlocharlie/epicgames-freegames:dev --target deploy",
    "docker:run": "docker run --rm -ti -v $(pwd)/config:/usr/app/config -p 3000:3000 -p 3001:3001 charlocharlie/epicgames-freegames-node:latest",
    "docker:run-debian": "docker run --rm -ti -v $(pwd)/config:/usr/app/config -p 3000:3000 -p 3001:3001 charlocharlie/epicgames-freegames-node:bullseye-slim",
    "docs": "typedoc --out docs src/common/config/classes.ts && cp test/test.html docs"
  },
  "dependencies": {
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^8.2.0",
    "filenamify": "^4.1.0",
    "fs-extra": "^10.0.0",
    "got": "^11.8.2",
    "jsdom": "^16.7.0",
    "json5": "^2.2.0",
    "localtunnel": "^2.0.2",
    "nodemailer": "^6.4.16",
    "object-assign-deep": "^0.4.0",
    "open": "^8.2.1",
    "otpauth": "^5.0.6",
    "pino": "^5.17.0",
    "pino-pretty": "^3.6.1",
    "puppeteer": "~10.4.0",
    "puppeteer-extra": "^3.2.3",
    "puppeteer-extra-plugin-portal": "^3.0.0",
    "puppeteer-extra-plugin-stealth": "^2.9.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "set-cookie-parser": "^2.4.3",
    "source-map-support": "^0.5.19",
    "tough-cookie": "^4.0.0",
    "tough-cookie-file-store": "^2.0.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^27.0.2",
    "@types/jsdom": "^16.2.13",
    "@types/json5": "^2.2.0",
    "@types/localtunnel": "^2.0.1",
    "@types/node": "^14.17.11",
    "@types/nodemailer": "^6.4.0",
    "@types/object-assign-deep": "^0.4.0",
    "@types/pino": "^5.15.5",
    "@types/set-cookie-parser": "^2.4.1",
    "@types/tough-cookie-file-store": "^2.0.1",
    "@types/yargs": "^15.0.4",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "copyfiles": "^2.4.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.1",
    "prettier": "^2.4.1",
    "randexp": "^0.5.3",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.4",
    "typescript": "^4.4.3",
    "yargs": "^16.0.3"
  },
  "engines": {
    "node": "14"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/claabs/epicgames-freegames-node.git"
  },
  "author": "Charlie Laabs",
  "eslintIgnore": [
    "**/*.config.js",
    "**/node_modules/**",
    "dist/**",
    "**/*.d.ts"
  ],
  "license": "MIT"
}
